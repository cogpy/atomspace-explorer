{
  "name": "atomspace-explorer",
  "version": "0.22.01",
  "license": "Apache-2.0",
  "angular-cli": {},
  "main": "electron.js",
  "scripts": {
    "ng": "NODE_OPTIONS='--openssl-legacy-provider' ng",
    "start": "NODE_OPTIONS='--openssl-legacy-provider' ng serve",
    "build": "NODE_OPTIONS='--openssl-legacy-provider' ng build",
    "build:prod": "NODE_OPTIONS='--openssl-legacy-provider' ng build --prod --base-href ./",
    "test": "NODE_OPTIONS='--openssl-legacy-provider' ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "electron": "electron .",
    "electron:build": "npm run build:prod && electron .",
    "pack": "npm run build:prod && electron-builder --dir",
    "dist": "npm run build:prod && electron-builder",
    "dist:mac": "npm run build:prod && electron-builder --mac",
    "dist:win": "npm run build:prod && electron-builder --win",
    "dist:linux": "npm run build:prod && electron-builder --linux"
  },
  "private": true,
  "overrides": {
    "@types/vinyl": "2.0.2",
    "@types/sizzle": "2.3.2",
    "@types/node": "8.0.47",
    "@types/d3-force": "1.2.0",
    "@types/d3-scale": "1.0.12",
    "@types/d3-zoom": "1.7.2",
    "@types/d3-array": "1.2.6",
    "@types/d3-dispatch": "1.0.8",
    "@types/d3-dsv": "1.0.34",
    "@types/geojson": "7946.0.7",
    "@types/debug": "0.0.30",
    "@types/ms": "0.7.30",
    "@types/yargs": "11.1.7",
    "@types/keyv": "3.1.1",
    "@types/minimatch": "3.0.3",
    "node-sass": "9.0.0"
  },
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/cdk": "^5.0.0-rc.1",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/flex-layout": "2.0.0-beta.10-4905443",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/material": "^5.0.0-rc.1",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/platform-server": "^5.0.2",
    "@angular/router": "^5.0.2",
    "angular-2-local-storage": "^1.0.1",
    "core-js": "^2.5.1",
    "d3": "^4.11.0",
    "d3-context-menu": "^0.1.2",
    "gulp": "^3.8.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "material-design-lite": "^1.3.0",
    "md-collapse": "0.0.1-alpha.1",
    "md2-accordion": "0.0.1-alpha.0",
    "ng2-atomspace-visualizer": "^1.3.7",
    "ng2-toastr": "^4.1.2",
    "ngx-uploader": "^4.0.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "^5.0.2",
    "@types/d3": "4.12.0",
    "@types/jasmine": "2.8.2",
    "@types/jquery": "3.2.17",
    "@types/node": "8.0.47",
    "codelyzer": "^4.0.1",
    "electron": "^22.3.27",
    "electron-builder": "^23.6.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "~5.2.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "3.3.0",
    "tslint": "^5.7.0",
    "typescript": "~2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "build": {
    "appId": "org.opencog.atomspace-explorer",
    "productName": "AtomSpace Explorer",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron.js",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "src/assets/icons/icon.icns",
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "src/assets/icons/icon.png",
      "category": "Development"
    }
  }
}
